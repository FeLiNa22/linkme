{
  "name": "@local/frontend",
  "type": "module",
  "scripts": {
    "codegen": "npm run lang:sync && npm run db:sync",
    "clean": "rimraf -g \"{.next,node_modules,.cache,**/__generated__}\"",
    "build": "npm run codegen && next build",
    "dev": "concurrently --kill-others 'next dev -p 4000' 'nodemon -w \"src/locales/**/*.json\" -x \"npm run lang:sync\"' 'nodemon -w \"../db/schema.prisma\" -x \"npm run db:sync\"'",
    "start": "next start",
    "lint": "npm run codegen && npm run lang:lint && eslint \"src/**/*.{ts,tsx}\"",
    "fix": "npm run codegen && eslint --fix \"src/**/*.{ts,tsx}\"",
    "db:sync": "prisma generate --schema=../db/schema.prisma",
    "lang:sync": "node scripts/lang-sync-missing.cjs && node scripts/lang-sync-inlang.cjs && npm run lang:type-gen",
    "lang:type-gen": "i18next-resources-for-ts interface -i ./src/locales/en -o ./src/@types/resources.d.ts",
    "lang:lint": "npx @inlang/cli lint --project ./project.inlang/"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.5.0",
    "@prisma/client": "^5.11.0",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@tanstack/react-query": "^5.25.0",
    "@tanstack/react-query-devtools": "^5.28.4",
    "@trpc/client": "^11.0.0-next-beta.311",
    "@trpc/react-query": "^11.0.0-next-beta.308",
    "@trpc/server": "^11.0.0-next-beta.311",
    "@vercel/otel": "^1.4.0",
    "accept-language": "^3.0.18",
    "axios": "^1.6.8",
    "dayjs": "^1.11.6",
    "i18next": "^23.10.0",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-resources-to-backend": "^1.2.0",
    "jotai": "^2.7.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "mongoose": "^8.2.2",
    "next": "^14.1.0",
    "next-auth": "^5.0.0-beta.15",
    "openapi-fetch": "^0.9.3",
    "posthog-js": "^1.116.0",
    "posthog-node": "^3.6.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@inlang/cli": "^2.12.4",
    "@swc-jotai/debug-label": "^0.1.0",
    "@swc-jotai/react-refresh": "^0.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/lodash": "^4.17.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "daisyui": "^4.7.3",
    "eslint-plugin-i18next": "^6.0.3",
    "i18next-locales-sync": "^2.0.1",
    "i18next-resources-for-ts": "^1.5.0",
    "nodemon": "^3.1.0",
    "openapi-typescript": "^7.0.0-next.8",
    "postcss": "^8.4.35",
    "prisma": "^5.11.0",
    "tailwindcss": "^3.4.1",
    "translation-check": "^1.1.0",
    "typescript": "^5.3.3",
    "yargs": "^17.7.2"
  }
}
